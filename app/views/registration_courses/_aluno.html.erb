<table>
  <thead>
    <tr>
      <th>Admission date</th>
      <th>Locked</th>
      <th>Conclusion date</th>
      <th>Curso</th>
      <th>Aluno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_courses.each do |registration_course| %>
      <tr>
        <td><%= registration_course.admission_date %></td>
        <td><%= registration_course.locked %></td>
        <td><%= registration_course.conclusion_date %></td>
        <td><%= registration_course.curso.nome %></td>
        <td><%= registration_course.aluno.nome %></td>
        <td><%= link_to 'Show', registration_course %></td>
        <td><%= link_to 'Edit', edit_registration_course_path(registration_course) %></td>
        <td><%= link_to 'Destroy', registration_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration course', new_registration_course_path %>